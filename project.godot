; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "EditorVCSInterface",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://addons/godot-git-plugin/git_api.gdns"
} ]
_global_script_class_icons={
"GitAPI": ""
}

[application]

config/name="ssjAug2022"
run/main_scene="res://scenes/main.tscn"
config/icon="res://icon.png"

[autoload]

EngineManager="*res://scenes/managers/engine_manager.tscn"
ResourceManager="*res://scripts/Managers/ResourceManager.cs"
LoadingScreen="*res://scenes/loading_screen.tscn"
SceneManager="*res://scenes/managers/scene_manager.tscn"
GameManager="*res://scenes/managers/game_manager.tscn"
GameUiManager="*res://scenes/managers/game_ui_manager.tscn"
PlayerManager="*res://scenes/managers/player_manager.tscn"
NpcManager="*res://scenes/managers/npc_manager.tscn"
ViewerManager="*res://scenes/managers/viewer_manager.tscn"
SpawnManager="*res://scenes/managers/spawn_manager.tscn"
DebugMenu="*res://scenes/debug_menu.tscn"

[display]

window/size/width=1280
window/size/height=720

[editor]

version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

[gui]

common/drop_mouse_on_gui_input_disabled=true

